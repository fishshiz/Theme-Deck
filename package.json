{
  "name": "theme-deck",
  "displayName": "Theme Deck",
  "description": "Automatically shuffle through editor themes on vscode.",
  "version": "0.0.1",
  "publisher": "edanlewis",
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.themeDeck"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Interval configuration",
      "properties": {
        "themeDeck.intervalTime": {
          "type": "number",
          "default": 20,
          "description": "Length of time in minutes between theme shuffles."
        }
      }
    },
    "commands": [
      {
        "command": "extension.themeDeck",
        "title": "Shuffle Themes"
      }
    ]
  },
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
